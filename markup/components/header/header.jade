                                            span.menu-mobile
                                                span.menu-mobile-hamburger.one
                                                span.menu-mobile-hamburger.two
                                                span.menu-mobile-hamburger.three
                                .header-mobile__content-cell
                                    .text-center
                                        a(href='#').header-mobile__phone 8-800-555-47-57

                            .header-mobile__navigation(data-for="header-bottom-toggleMenuMbles")
                                .header-mobile__navigation-content
                                    .header-mobile__navigation-list
                                        ul
                                            li: a(href="#") Каталог камня
                                            li: a(href="#") Изделия
                                            li: a(href="#") Услуги
                                            li: a(href="#") Галерея работ
                                            li: a(href="#") О компании
                                            li: a(href="#") Акции
                                            li: a(href="#") Контакты

                .hidden-md-down
                    .header__fix
                        .header__table
                            .header__table-cell.header-desktop
